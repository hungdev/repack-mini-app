{
  "name": "RepackMiniApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android --port 9000",
    "ios": "react-native run-ios --port 9000",
    "lint": "eslint .",
    "start": "react-native start --port 9000",
    "test": "jest",
    "bundle": "npm run bundle-ios && npm run bundle-android",
    "bundle-ios": "react-native bundle --config rspack.config.mjs --platform ios --entry-file index.js --dev=false --bundle-output build/generated/ios/index.bundle --assets-dest build/generated/ios",
    "bundle-android": "react-native bundle --config rspack.config.mjs --platform android --entry-file index.js --dev=false --bundle-output build/generated/android/index.bundle --assets-dest build/generated/android"
  },
  "dependencies": {
    "@ant-design/icons-react-native": "2.3.2",
    "@ant-design/react-native": "5.4.3",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-documents/picker": "10.1.5",
    "@react-native-documents/viewer": "1.0.1",
    "@react-native-vector-icons/common": "12.0.0",
    "@react-native-vector-icons/fontawesome6": "12.0.0",
    "@react-native-vector-icons/ionicons": "12.3.0",
    "@react-navigation/bottom-tabs": "7.3.13",
    "@react-navigation/native": "7.1.9",
    "@react-navigation/native-stack": "7.3.13",
    "@reduxjs/toolkit": "2.8.2",
    "axios": "1.9.0",
    "dayjs": "1.11.13",
    "i18next": "25.2.0",
    "immer": "10.1.1",
    "na-components": "file:../../na-components/na-components-1.0.4.tgz",
    "react": "19.0.0",
    "react-hook-form": "7.58.1",
    "react-i18next": "15.5.1",
    "react-native": "0.79.2",
    "react-native-config": "1.5.6",
    "react-native-element-dropdown": "2.12.4",
    "react-native-gesture-handler": "2.28.0",
    "react-native-linear-gradient": "2.8.3",
    "react-native-localize": "3.4.1",
    "react-native-pager-view": "6.8.1",
    "react-native-reanimated": "3.19.0",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "4.10.0",
    "react-native-size-matters": "0.4.2",
    "react-native-size-scaling": "0.5.4",
    "react-native-tab-view": "4.1.2",
    "react-redux": "9.2.0",
    "redux-persist": "6.0.0",
    "yup": "1.6.1"
